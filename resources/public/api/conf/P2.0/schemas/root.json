{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Individual's Income",
  "type": "object",
  "properties": {
    "_links": {
      "type": "object",
      "description": "A list of links for possible actions related to this endpoint.",
      "properties": {
        "paye": {
          "type": "object",
          "description": "The Uniform Resource Identifier (URI) for the endpoint to get an individualâ€™s income data per employment.",
          "properties": {
            "href": {
             "type": "string",
              "description": "The URL of the endpoint.",
              "example": "/individuals/income/paye?matchId=57072660-1df9-4aeb-b4ea-cd2d7f96e430{&startDate,endDate}"
            },
            "title": {
              "type": "string",
              "example": "Get an individual's income data per employment"
            }
          },
          "required": ["href", "title"]
        },
        "selfAssessment": {
          "type": "object",
          "description": "The URI for the endpoint to get an individual's Self Assessment income data.",
          "properties": {
            "href": {
              "type": "string",
              "description":"The URL of the endpoint.",
              "example": "/individuals/income/sa?matchId=57072660-1df9-4aeb-b4ea-cd2d7f96e430{&startYear,endYear}"
            },
            "title": {
              "type": "string",
              "example": "Get an individual's Self Assessment income data"
            }
          },
          "required": ["href", "title"]
        },
        "self": {
          "type": "object",
          "description": "The URI for this endpoint.",
          "properties": {
            "href": {
              "type": "string",
              "example": "/individuals/income/?matchId=57072660-1df9-4aeb-b4ea-cd2d7f96e430"
            }
          },
          "required": ["href"]
        }
      },
      "required": ["paye", "selfAssessment", "self"]
    }
  },
  "required": ["_links"]
}
