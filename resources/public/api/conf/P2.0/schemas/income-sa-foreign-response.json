{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "An individual’s Self Assessment foreign income data for a requested time period.",
  "type": "object",
  "properties": {
    "_links": {
      "type": "object",
      "description": "A list of links for possible actions related to this endpoint.",
      "properties": {
        "self": {
          "type": "object",
          "description": "The URI to this resource",
          "properties": {
            "href": {
              "type": "string",
              "description": "The URL of the endpoint.",
              "example": "/individuals/income/sa/foreign?matchId=57072660-1df9-4aeb-b4ea-cd2d7f96e430&startYear=2018-19&endYear=2019-20"
            }
          },
          "required": [
            "href"
          ]
        }
      },
      "required": [
        "self"
      ]
    },
    "selfAssessment": {
      "type": "object",
      "description": "An individual’s Self Assessment foreign income data for a requested time period.",
      "properties": {
        "taxReturns": {
          "type": "array",
          "description": "List of Self Assessment tax returns in the requested period",
          "items": {
            "type": "object",
            "properties": {
              "taxYear": {
                "type": "string",
                "description": "Tax year",
                "example": "2018-19"
              },
              "foreign": {
                "type": "array",
                "description": "Foreign income for the tax year",
                "items": {
                  "type": "object",
                  "properties": {
                    "foreignIncome": {
                      "type": "number",
                      "description": "Foreign income",
                      "example": "2143.32"
                    }
                  },
                  "required": [
                    "foreignIncome"
                  ]
                }
              }
            },
            "required": [
              "taxYear",
              "foreign"
            ]
          }
        }
      },
      "required": [
        "taxReturns"
      ]
    }
  },
  "required": [
    "_links",
    "selfAssessment"
  ]
}
