{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Income",
  "type": "object",
  "properties": {
    "_links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "object",
          "description": "The URI to this resource",
          "properties": {
            "href": {
              "type": "string",
              "example": "/individuals/income/paye?matchId=57072660-1df9-4aeb-b4ea-cd2d7f96e430&fromDate=2016-01-01&toDate=2017-03-01"
            }
          },
          "required": [
            "href"
          ]
        }
      },
      "required": [
        "self"
      ]
    },
    "paye": {
      "type": "object",
      "properties": {
        "income": {
          "type": "array",
          "description": "List of payments for each employment in the requested period",
          "items": {
            "type": "object",
            "properties": {
              "employerPayeReference": {
                "type": "string",
                "description": "Employer PAYE Reference",
                "example": "123/AI45678"
              },
              "taxYear": {
                "type": "string",
                "description": "The tax year of submission, format YY-YY.",
                "example": "2018-19"
              },
              "employee": {
                "description": "A collection of properties related to the employee in the submission",
                "type": "object",
                "properties": {
                  "hasPartner": {
                    "type": "boolean",
                    "description": "Will only be true if the partner object is present",
                    "example": "true"
                  }
                }
              },
              "payroll": {
                "description": "Properties relevant to the employee's payroll",
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string",
                    "description": "Identifier assigned by the employer to the individual. Pattern: ^[^ ].{0,34}$",
                    "example": "yxz8Lt5?/`/>6]5b+7%>o-y4~W5suW"
                  }
                }
              },
              "payFrequency": {
                "description": "The frequency of payments. W1 Weekly, W2 Fortnightly, W4 Four weekly, M1 Monthly, M3 Quarterly, M6 Bi-annually, MA Annually, IO One off, IR Irregular",
                "type": "string",
                "example": "W1"
              },
              "weeklyPayNumber": {
                "description": "If paid weekly, the period number. Range 1 - 54, and 56. Pattern: ^([1-9]|[1-4][0-9]|5[0-46])$",
                "type": "string",
                "example": "1"
              },
              "monthlyPayNumber": {
                "description": "If paid monthly, the period number. Range 1 - 12. Pattern: ^([1-9]|1[0-2])$",
                "type": "string",
                "example": "12"
              },
              "paymentDate": {
                "type": "string",
                "format": "date",
                "description": "Payment date",
                "example": "2016-02-28"
              }
            }
          }
        }
      },
      "required": [
        "income"
      ]
    }
  },
  "required": [
    "_links",
    "paye"
  ]
}
