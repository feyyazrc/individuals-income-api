{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The individual's SA income for the given period",
  "type": "object",
  "properties": {
    "_links": {
      "type": "object",
      "description": "A list of links for possible actions related to this endpoint.",
      "properties": {
        "self": {
          "type": "object",
          "description": "The Uniform Resource Identifier (URI) for this endpoint.",
          "properties": {
            "href": {
              "type": "string",
              "description": "The URL of the endpoint.",
              "example": "/individuals/income/sa?matchId=57072660-1df9-4aeb-b4ea-cd2d7f96e430{&fromTaxYear,toTaxYear}"
            }
          },
          "required": [
            "href"
          ]
        },
        "additionalInformation": {
          "type": "object",
          "description": "The URI for the endpoint to get an individual's additional Self Assessment information for the requested time period.",
          "properties": {
            "href": {
              "type": "string",
              "description": "The URL of the endpoint.",
              "example": "/individuals/income/sa/additional-information?matchId=57072660-1df9-4aeb-b4ea-cd2d7f96e430{&fromTaxYear,toTaxYear}"
            },
            "title": {
              "type": "string",
              "description": "Get an individual's SA additional information data"
            }
          },
          "required": [
            "href",
            "title"
          ]
        },
        "employments": {
          "type": "object",
          "description": "The URI for the endpoint to get an individual's Self Assessment employments income data.",
