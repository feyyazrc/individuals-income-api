            }
          },
          "required": [
            "href",
            "title"
          ]
        }
      },
      "required": [
        "self"
      ]
    },
    "selfAssessment": {
      "type": "object",
      "description": "Self Assessment tax return history for a requested time period.",
      "properties": {
        "registrations": {
          "type": "array",
          "description": "Self Assessment registrations.",
          "items": {
            "type": "object",
            "properties": {
              "registrationDate": {
                "type": "string",
                "description": "Self Assessment registration date. Date in format YYY-MM-DD",
                "example": "2012-01-06"
              }
            }
          }
        },
        "taxReturns": {
          "type": "array",
          "description": "A list of Self Assessment tax returns in the requested period.",
          "items": {
            "type": "object",
            "properties": {
              "taxYear": {
                "type": "string",
                "description": "The tax year in the format YYY-YY.",
                "example": "2015-16"
              },
              "submissions": {
                "type": "array",
                "description": "The date the tax return was received. Date in the format: YYYY-MM-DD",
                "items": {
                  "type": "object",
                  "properties": {
                    "receivedDate": {
                      "type": "string",
                      "description": "Tax return received date",
                      "example": "2014-06-06"
                    }
                  }
                }
              }
            },
            "required": [
              "taxYear",
              "submissions"
            ]
          }
        }
      },
      "required": [
        "registrations",
        "taxReturns"
      ]
    }
  },
  "required": [
    "_links",
    "selfAssessment"
  ]
}
