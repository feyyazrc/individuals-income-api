          "properties": {
            "href": {
              "type": "string",
              "description": "The URL of the endpoint.",
              "example": "/individuals/income/sa/further-details?matchId=57072660-1df9-4aeb-b4ea-cd2d7f96e430{&fromTaxYear,toTaxYear}"
            },
